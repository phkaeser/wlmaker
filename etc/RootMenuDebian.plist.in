// Definition of root menu, for Debian with wmmenugen(1).
("Root Menu",
 //! [wmmenugen]
 ("Applications...", GeneratePlistMenu, "@wmmenugen_executable@ -parser:xdg /usr/share/applications/"),
 //! [wmmenugen]
 ("Workspaces...",
  ("New", WorkspaceAdd),
  ("Destroy Last", WorkspaceDestroyLast),
  ("Go To Previous", WorkspacePrevious),
  ("Go To Next", WorkspaceNext)),
 ("Lock", LockScreen),
 (Exit, Quit))
